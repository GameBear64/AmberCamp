{
  "name": "ambercamp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently -n BE,FE -c yellow,blue \"npm run BE\" \"npm run FE\"",
    "BE": "cd server && nodemon",
    "FE": "vite",
    "lineStats": "cloc --fullpath --not-match-d=\"(node_modules|App/ios|App/android)\" --not-match-f=\"(yarn.lock|package.json|pnpm-lock.yaml|package-lock.json)\" .",
    "build": "vite build",
    "preview": "vite preview",
    "prettier": "prettier --write \"src/**/*.{js,jsx}\" \"server/**/*.js\"",
    "eslint": "eslint --fix \"src/**/*.{js,jsx}\" \"server/*.js\"",
    "lint": "npm run prettier && npm run eslint"
  },
  "dependencies": {
    "@nanostores/persistent": "^0.10.1",
    "@nanostores/react": "^0.7.1",
    "awesome-snackbar": "^2.0.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "coupon-code": "^0.4.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-file-routing": "^2.1.5",
    "express-throttle": "^2.0.0",
    "ffmpeg-static": "^5.1.0",
    "glob": "^10.3.6",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.11.2",
    "morgan": "^1.10.0",
    "nanostores": "^0.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-loading-skeleton": "^3.3.1",
    "react-markdown": "^8.0.7",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.14.0",
    "react-twemoji": "^0.5.0",
    "referral-codes": "^2.0.3",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.31.3",
    "simple-thumbnail": "^1.6.5",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.2",
    "spark-md5": "^3.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "nodemon": "^2.0.22",
    "prettier": "2.8.4",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1"
  }
}
